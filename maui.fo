      <fo:block space-before.minimum="0em" space-before.optimum="0em" space-before.maximum="0em" space-after.minimum="0.8em" space-after.optimum="1em" space-after.maximum="1.2em" hyphenate="false" wrap-option="no-wrap" white-space-collapse="false" white-space-treatment="preserve" linefeed-treatment="preserve" text-align="start" border-color="#151B54" border-style="solid" border-width="0.2pt" padding-top="0.5em" padding-left="0.3em" padding-right="0.3em" padding-bottom="0.5em" margin-left="0em" margin-right="0em" font-family="monospace" font-size="7.8pt" background-color="#E3E4FA" id="d5e124">.
|-- pom.xml
`-- src
    |-- main
    |   `-- java
    |       `-- com
    |           `-- soebes
    |               `-- maui
    |                   `-- it
    |                       `-- BitMask.java
    `-- test
        `-- java
            `-- com
                `-- soebes
                    `-- maui
                        `-- it
